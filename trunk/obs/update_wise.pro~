pro update_wise

data=mrdfits('test/test.fits')
help,data,/struct
f1=data[0,*]
f2=data[1,*]
f3=data[2,*]
f4=data[3,*]
print,min(f1),min(f2),min(f3),min(f4)
plot,f1,f2,psym=3,/xlog,/ylog
header=headfits('test/test.fits')

sxaddpar,header,'W1_FMIN',0.02,'Minimum flux1, these data'
sxaddpar,header,'W2_FMIN',0.03,'Minimum flux2, these data'
sxaddpar,header,'W3_FMIN',2.4,'Minimum flux3, these data'
sxaddpar,header,'W4_FMIN',63.0,'Minimum flux4, these data'
sxaddpar,header,'W1_FERR',0.0067,'rms1, these data 3sig=min_f'
sxaddpar,header,'W2_FERR',0.02,'rms2, these data 3sig=min_f'
sxaddpar,header,'W3_FERR',0.8,'rms3, these data 3sig=min_f'
sxaddpar,header,'W4_FERR',21.,'rms4, these data 3sig=min_f'


writefits,'wise.fits',data,header
end
